!--Homepage-->
<div class="container">
    <div class="col-md-12 jumbotron">
        <div class="text-center">
            <h1> New York Times </h1>
      
        {{#if article}}
            {{#each article}}
          
                    <h4>{{title}}</h4>
               
                    <p> {{summary}}</p>
                    <a href={{link}} target="_blank">{{link}}</a>
               
            {{/each}}
        
        {{/if}}
    </div>
</div>